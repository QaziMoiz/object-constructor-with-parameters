var Car = function (wheels, engine, seats, manufacturer){
  this.wheels = wheels;
  this.engine = engine;
  this.seats = seats;
  this.manufacturer = manufacturer;
}

var myNewCar = new Car(4,1,8,"Land Cruiser");
console.log(myNewCar);
/*Object {
  engine: 1,
  manufacturer: "Land Cruiser",
  seats: 8,
  wheels: 4
}*/

var myOldCar = new Car(3,1,3,"Tuk Tuk");
console.log(myOldCar); 
/*Object {
  engine: 1,
  manufacturer: "Tuk Tuk",
  seats: 3,
  wheels: 3
}*/
